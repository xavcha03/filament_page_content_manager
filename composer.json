{
  "name": "xavcha/page-content-manager",
  "description": "Professional Laravel Filament package for managing pages with flexible content blocks system",
  "type": "library",
  "license": "MIT",
  "version": "0.2.0",
  "require": {
    "php": "^8.2",
    "filament/filament": "^4.5",
    "illuminate/database": "^12.0",
    "illuminate/filesystem": "^12.0",
    "illuminate/routing": "^12.0",
    "illuminate/support": "^12.0",
    "xavcha/fillament-xavcha-media-library": "dev-main"
  },
  "require-dev": {
    "orchestra/testbench": "^10.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Xavcha\\PageContentManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Xavcha\\PageContentManager\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Xavcha\\PageContentManager\\PageContentManagerServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
